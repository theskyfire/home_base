##############################################################################

export HOMEbase="${HOME}/base"
export HB="${HOMEbase}"
export HB_USER="${USER}"
: ${HB_USER:=${USERNAME}}
: ${HB_USER:=$(whoami)}
export HB_HOME="${HOME}"
: ${HB_HOME:=~}
: ${HB_HOME:=$(cd ; pwd)}

export HB_BIN="${HB}/bin"
export HB_SBIN="${HB}/sbin"
export HB_TMP="${HB}/tmp"
export HB_ETC="${HB}/etc"

export HB_PATH="${HB_SBIN}:${HB_BIN}"
export PATH="${HB_PATH}:${PATH}"

export HB_INPUTRC="${HB_ETC}/inputrc"
export INPUTRC="${HB_INPUTRC}"

export HB_BASH="${HB_BIN}/bash"
export HB_BASHRC="${HB_ETC}/bashrc"
export HB_SHELL="${HB_BASH}"
export SHELL="${HB_SHELL}"

# export HB_PAGER="${HB_BIN}/less"
export HB_PAGER="less"
export PAGER="${HB_PAGER}"

export HB_VIM="${HB_BIN}/vim"
export HB_VIMRC="${HB_ETC}/vimrc"
export HB_EDITOR="${HB_VIM}"
export EDITOR="${HB_EDITOR}"

export HB_MANPATH="${HB}/man"
export MANPATH="${HB_MANPATH}:${MANPATH}"

##############################################################################
unset CDPATH
